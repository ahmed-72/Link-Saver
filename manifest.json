{
    "manifest_version": 3,
    "name": "Leads Tracker",
    "description": "Save links and tabs to revisit later.",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  